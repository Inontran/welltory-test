{
  "name": "welltory-test",
  "version": "1.0.0",
  "description": "Тестовое задание для Welltory.",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development webpack serve --mode development",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "eslint:fix": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix",
    "gh-deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inontran/welltory-test.git"
  },
  "author": "alexandr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Inontran/welltory-test/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.7",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^4.0.2",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "4.2",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^4.5.2",
    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "pug": "^2.0.0",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "terser-webpack-plugin": "^3.1.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "homepage": "https://github.com/Inontran/welltory-test#readme",
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
